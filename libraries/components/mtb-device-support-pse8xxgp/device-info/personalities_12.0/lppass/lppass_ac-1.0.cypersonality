        <Entry name="SAR FIFO 1 Level"      value="CY_AUTANALOG_AC_OUT_TRIG_MASK_FIFO_LEVEL1"   visible="true"/>
        <Entry name="SAR FIFO 2 Level"      value="CY_AUTANALOG_AC_OUT_TRIG_MASK_FIFO_LEVEL2"   visible="true"/>
        <Entry name="SAR FIFO 3 Level"      value="CY_AUTANALOG_AC_OUT_TRIG_MASK_FIFO_LEVEL3"   visible="true"/>
        <Entry name="SAR FIFO 4 Level"      value="CY_AUTANALOG_AC_OUT_TRIG_MASK_FIFO_LEVEL4"   visible="true"/>
        <Entry name="SAR FIFO 5 Level"      value="CY_AUTANALOG_AC_OUT_TRIG_MASK_FIFO_LEVEL5"   visible="true"/>
        <Entry name="SAR FIFO 6 Level"      value="CY_AUTANALOG_AC_OUT_TRIG_MASK_FIFO_LEVEL6"   visible="true"/>
        <Entry name="SAR FIFO 7 Level"      value="CY_AUTANALOG_AC_OUT_TRIG_MASK_FIFO_LEVEL7"   visible="true"/>
        <Entry name="PTComp Comparator 0"   value="CY_AUTANALOG_AC_OUT_TRIG_MASK_PTCOMP_CMP0"   visible="true"/>
        <Entry name="PTComp Comparator 1"   value="CY_AUTANALOG_AC_OUT_TRIG_MASK_PTCOMP_CMP1"   visible="true"/>
        <Entry name="PTComp Post Process 0" value="CY_AUTANALOG_AC_OUT_TRIG_MASK_PTCOMP_RANGE0" visible="true"/>
        <Entry name="PTComp Post Process 1" value="CY_AUTANALOG_AC_OUT_TRIG_MASK_PTCOMP_RANGE1" visible="true"/>
        <Entry name="DAC 0 Range 0"         value="CY_AUTANALOG_AC_OUT_TRIG_MASK_DAC0_RANGE0"   visible="true"/>
        <Entry name="DAC 0 Range 1"         value="CY_AUTANALOG_AC_OUT_TRIG_MASK_DAC0_RANGE1"   visible="true"/>
        <Entry name="DAC 0 Range 2"         value="CY_AUTANALOG_AC_OUT_TRIG_MASK_DAC0_RANGE2"   visible="true"/>
