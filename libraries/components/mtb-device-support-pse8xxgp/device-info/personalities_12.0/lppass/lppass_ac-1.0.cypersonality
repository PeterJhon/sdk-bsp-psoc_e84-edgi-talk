        <Entry name="Autonomous Controller" value="CY_AUTANALOG_AC_OUT_TRIG_MASK_AC"            visible="true"/>
        <Entry name="SAR Done"              value="CY_AUTANALOG_AC_OUT_TRIG_MASK_SAR_DONE"      visible="true"/>
        <Entry name="SAR EOS"               value="CY_AUTANALOG_AC_OUT_TRIG_MASK_SAR_EOS"       visible="true"/>
        <Entry name="SAR Range 0"           value="CY_AUTANALOG_AC_OUT_TRIG_MASK_SAR_RANGE0"    visible="true"/>
        <Entry name="SAR Range 1"           value="CY_AUTANALOG_AC_OUT_TRIG_MASK_SAR_RANGE1"    visible="true"/>
