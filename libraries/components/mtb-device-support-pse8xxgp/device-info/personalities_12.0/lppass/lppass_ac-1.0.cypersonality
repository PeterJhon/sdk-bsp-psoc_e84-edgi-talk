        <Entry name="DAC 1 Range 2"         value="CY_AUTANALOG_AC_OUT_TRIG_MASK_DAC1_RANGE2"   visible="true"/>
        <Entry name="DAC 0 Epoch"           value="CY_AUTANALOG_AC_OUT_TRIG_MASK_DAC0_EPOCH"    visible="true"/>
        <Entry name="DAC 1 Epoch"           value="CY_AUTANALOG_AC_OUT_TRIG_MASK_DAC1_EPOCH"    visible="true"/>
        <Entry name="CTB 0 Comparator 0"    value="CY_AUTANALOG_AC_OUT_TRIG_MASK_CTB0_COMP0"    visible="true"/>
        <Entry name="CTB 0 Comparator 1"    value="CY_AUTANALOG_AC_OUT_TRIG_MASK_CTB0_COMP1"    visible="true"/>
        <Entry name="CTB 1 Comparator 0"    value="CY_AUTANALOG_AC_OUT_TRIG_MASK_CTB1_COMP0"    visible="true"/>
        <Entry name="CTB 1 Comparator 1"    value="CY_AUTANALOG_AC_OUT_TRIG_MASK_CTB1_COMP1"    visible="true"/>
        <Entry name="None"                  value="CY_AUTANALOG_AC_OUT_TRIG_MASK_EMPTY"         visible="true"/>
      </ParamChoice>
    </Repeat>

