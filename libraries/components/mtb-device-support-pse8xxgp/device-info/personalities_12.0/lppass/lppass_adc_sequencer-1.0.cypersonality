                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX8)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX8&quot;  :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX9)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX9&quot;  :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX10) ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX10&quot; :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX11) ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX11&quot; :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX12) ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX12&quot; :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX13) ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX13&quot; :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX14) ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX14&quot; :
                    (channelGpio eq CY_AUTANALOG_SAR_CHAN_CFG_MUX15) ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX15&quot; :
                     &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX0&quot;}`" />

    <ParamString id="simultaneousMux0Sel" name="simultaneousMux0Sel" group="Internal" visible="`${debug}`" desc="" editable="false"
        default="`${!channelMux0 ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX0&quot; :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX0)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX0&quot;  :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX1)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX1&quot;  :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX2)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX2&quot;  :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX3)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX3&quot;  :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX4)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX4&quot;  :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX5)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX5&quot;  :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX6)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX6&quot;  :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX7)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX7&quot;  :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX8)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX8&quot;  :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX9)  ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX9&quot;  :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX10) ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX10&quot; :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX11) ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX11&quot; :
                    (sar_mux_0Choice eq CY_AUTANALOG_SAR_CHAN_CFG_MUX12) ? &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX12&quot; :
