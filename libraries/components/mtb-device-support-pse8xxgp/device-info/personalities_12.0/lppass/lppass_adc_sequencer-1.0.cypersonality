    <ParamString id="nonSimultaneousSamplingGPIOch" name="nonSimultaneousSamplingGPIOch" group="Internal" visible="`${debug}`" desc="" editable="false"
        default="`${(channelGpio eq CY_AUTANALOG_SAR_CHAN_MASK_GPIO0) ? &quot;CY_AUTANALOG_SAR_CHAN_MASK_GPIO0&quot; :
