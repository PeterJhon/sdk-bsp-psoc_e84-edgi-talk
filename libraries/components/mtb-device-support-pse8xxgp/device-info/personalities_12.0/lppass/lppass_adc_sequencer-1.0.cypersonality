  <DRCs>
    <DRC type="ERROR" text="Parent resource for ADC Scans should be enabled and configured." condition="`${!isBlockUsed(resource_sar)}`" >
      <FixIt action="ENABLE_BLOCK" target="`${resource_sar}`" value="lppass_adc-1.0" valid="true" />
    </DRC>
    <DRC type="ERROR"
         text="For the SAR ADC in High Speed mode, either MUX or GPIO channel should be selected in the scan."
         condition="`${((channelGpioEnable || channelMuxEnable) &amp;&amp; (operatingMode eq &quot;HighSpeed&quot;) &amp;&amp; (!simultaneousSampling)) &amp;&amp; (channelGpio eq CY_AUTANALOG_SAR_CHAN_MASK_GPIO_DISABLED)}`"/>
    <DRC type="ERROR"
         text="For the SAR ADC in High Speed mode, MUX1 cannot be used because Mux0 is configured for the channel in pseudo-differential mode."
         condition="`${(operatingMode eq &quot;HighSpeed&quot;) &amp;&amp; (simultaneousMuxMode eq &quot;CY_AUTANALOG_SAR_CHAN_CFG_MUX0_PSEUDO_DIFF&quot;) &amp;&amp; channelMux1}`"/>
